 <div class="container"> <!--Title container -->
        <div class="row">
            <div class="col s12 center-align"><h1>Abc News Scraper</h1></div>
        </div>
    </div>
    
    <div class="container"> <!--body container-->
        <div class="row">
            <div class="col s12 center-align"><h2>Articles</h2></div>
        </div>
        <div class="row" >
            <div class ="col s6 center-align" id="articleHolder">
                {{#each article}}
                    `<div class="card">
                        <div class="card-image waves-effect waves-block waves-light">
                          <img class="activator" src="{{this.imgLink}}">
                        </div>
                        <div class="card-content">
                          <span class="card-title activator grey-text text-darken-4">{{this.title}}<i class="material-icons right">more_vert</i></span>
                          <p><a href={{this.link}}>This is a link to the article</a></p>
                        </div>
                        <div class="card-reveal">
                          <span class="card-title grey-text text-darken-4">{{this.title}}<i class="material-icons right">close</i></span>
                          <p>{{this.content}}</p>
                          <div class="input-field col s12">
                              <input id="{{this.id}}" type="text" class="validate">
                              <label for="{{this.id}}">Add a note!</label>
                          </div>
                          <div class="center-align">Notes</div>
                          <div id="note{{this.id}}">
                              {{#if this.note.body}}{{this.note.body}}{{/if}}
                          </div>
                          <a class="btn-floating halfway-fab waves-effect waves-light red" data-id="{{this.id}}" onclick="submitNote('{{this.id}}')"><i class="material-icons">add</i></a>
                        </div>
                    </div>
                {{/each}}
            </div>
            <div class ="col s6 center-align" id="articleHolder2">
                
            </div>
        </div>
    </div>
    
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script type="text/javascript" src="js/materialize.min.js"></script>
    <script type ="text/javascript" src="appjs.js">    </script>
     <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>